{
  "path": {
    "ffmpeg": "ffmpeg",
    "input": "/in/",
    "output": "/out/"
  },
  "audio": {
    "bitrate_type": "cbr",
    "bitrate": 320,
    "convert_extensions": "flac, wav",
    "output_codec": "aac",
    "output_sample_rate": null
  },
  "advance": {
    "custom_command_enabled": false,
    "custom_command": "ffmpeg -i [INPUT] -acodec alac [OUTPUT]",
    "ignore_extensions": ["7z", "zip"],
    "match_method": "date_size",
    "thread_count": "max"
  }
}
